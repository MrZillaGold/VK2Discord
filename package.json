{
  "name": "vk2discord",
  "version": "2.0.0",
  "LATEST_CONFIG_VERSION": 4,
  "description": "Автоматическая публикация постов из VK.COM в канал Discord",
  "main": "index.mjs",
  "scripts": {
    "start": "npm run check && npm run init",
    "check": "node --experimental-modules --experimental-json-modules --es-module-specifier-resolution=node ./scripts/check.mjs",
    "init": "node --experimental-modules --experimental-json-modules --es-module-specifier-resolution=node index.mjs",
    "update": "node --experimental-modules --experimental-json-modules --es-module-specifier-resolution=node ./scripts/update.mjs",
    "test": "node --experimental-modules --experimental-json-modules node_modules/mocha/bin/_mocha --reporter spec --exit"
  },
  "homepage": "https://github.com/MrZillaGold/VK2Discord#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrZillaGold/VK2Discord.git"
  },
  "author": "MrZillaGold <Pronin Egor>",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/MrZillaGold/VK2Discord/issues"
  },
  "dependencies": {
    "discord.js": "^12.4.1",
    "string-replace-async": "^2.0.0",
    "vk-io": "^4.0.4"
  },
  "devDependencies": {
    "mocha": "^8.2.1"
  },
  "engines": {
    "node": ">=12.9.0"
  }
}
