  "bugs": {
    "url": "https://github.com/MrZillaGold/VK2Discord/issues"
  },
  "dependencies": {
    "string-replace-async": "^2.0.0",
    "vk-io": "^4.0.1",
    "webhook-discord": "^3.7.5"
  },
  "devDependencies": {
    "mocha": "^8.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
